<div class="authentication">
  <table>
    <%- @users.each do |u| -%>
      <tr>
        <td><%= u.email %></td>
        <td><%= u.created_at %></td>
        <td><%= link_to 'Log Out', destroy_user_session_path, :method => :delete %></td>
      </tr>
    <%- end -%>
</table>
</div>


